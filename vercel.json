{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/kitchinette/**"
    }
  }
}
