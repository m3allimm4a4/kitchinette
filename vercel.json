{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "framework": null,
  "builds": [
    {
      "src": "dist/kitchinette/server/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/kitchinette/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/kitchinette/server/main.js"
    }
  ]
}
